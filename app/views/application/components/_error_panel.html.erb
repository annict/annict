<% if resource.errors.any? %>
  <div class="alert alert-danger">
    <h4 class="alert-heading"></h4>
      <%= t "noun.error" %>
      <ul>
        <% resource.errors.full_messages.each do |msg| %>
          <li>
            <%= msg %>
          </li>
        <% end %>
      </ul>
  </div>
<% end %>
