ANNICT_ADMIN_IP= # メンテナンスモード時にアクセスを許可する管理者IP（カンマ区切りで複数指定可）。例: 192.168.1.1,10.0.0.1
ANNICT_COOKIE_DOMAIN=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_COOKIE_DOMAIN # 例: .example.com
ANNICT_DISABLE_RATE_LIMIT=true
ANNICT_DOMAIN=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_DOMAIN # 例: example.com
ANNICT_MAINTENANCE_MODE= # メンテナンスモードの有効/無効（"on" で有効）
ANNICT_IMGPROXY_ENDPOINT=http://imgproxy:8080
ANNICT_IMGPROXY_KEY=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_IMGPROXY_KEY
ANNICT_IMGPROXY_SALT=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_IMGPROXY_SALT
ANNICT_PORT=8080
ANNICT_RAILS_APP_URL=http://rails-app:3000
ANNICT_REDIS_URL=redis://redis:6379/0
ANNICT_RESEND_API_KEY=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_RESEND_API_KEY
ANNICT_RESEND_FROM_EMAIL=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_RESEND_FROM_EMAIL # 例: noreply@annict.com (メールアドレスのみ。差出人名 "Annict" はコード側で追加されます)
ANNICT_S3_ACCESS_KEY_ID=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_S3_ACCESS_KEY_ID
ANNICT_S3_BUCKET_NAME=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_S3_BUCKET_NAME
ANNICT_S3_ENDPOINT=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_S3_ENDPOINT
ANNICT_S3_REGION=auto
ANNICT_S3_SECRET_ACCESS_KEY=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_S3_SECRET_ACCESS_KEY
ANNICT_SESSION_HTTPONLY=true
ANNICT_SESSION_SECURE=false
ANNICT_TURNSTILE_SECRET_KEY=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_TURNSTILE_SECRET_KEY
ANNICT_TURNSTILE_SITE_KEY=op://VAULT_EXAMPLE/env_$APP_ENV/Go/ANNICT_TURNSTILE_SITE_KEY
ANNICT_SENTRY_DSN= # Sentry DSN（本番環境で必須）。例: https://xxx@xxx.ingest.sentry.io/xxx
ANNICT_SENTRY_ENVIRONMENT= # 環境名。空の場合はAPP_ENVの値を使用
ANNICT_SENTRY_TRACES_SAMPLE_RATE= # トレースサンプリングレート（0.0〜1.0）。デフォルト: 0.5
ANNICT_SENTRY_DEBUG= # デバッグモード（"true"で有効）。デフォルト: false
DATABASE_URL=op://VAULT_EXAMPLE/env_$APP_ENV/Go/DATABASE_URL # 例: postgres://postgres:password@postgresql:5432/annict_development?sslmode=disable
