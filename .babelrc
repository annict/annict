{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "last 2 versions",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ]
  ],

  "plugins": ["syntax-dynamic-import", "transform-object-rest-spread", ["transform-class-properties", { "spec": true }]]
}
